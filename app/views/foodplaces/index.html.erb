<p id="notice"><%= notice %></p>

<h1>Foodplaces</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Address</th>
      <th>Score</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foodplaces.each do |foodplace| %>
      <tr>
        <td><%= foodplace.name %></td>
        <td><%= foodplace.email %></td>
        <td><%= foodplace.address %></td>
        <td><%= foodplace.score %></td>
        <td><%= foodplace.phone %></td>
        <td><%= link_to 'Show', foodplace %></td>
        <td><%= link_to 'Edit', edit_foodplace_path(foodplace) %></td>
        <td><%= link_to 'Destroy', foodplace, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foodplace', new_foodplace_path %>
